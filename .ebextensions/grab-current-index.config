container_commands:
  00-list-env:
    command: "env"
  01-grab-index:
    command: "aws --region=us-west-2 s3 cp s3://solr.ucldc/indexes/production/solr-index.tar.bz2 /var/app/solr-index/solr-index.tar.bz2"
  02-extract-index-archive:
    command: "cd /var/app/solr-index; tar -xvf /var/app/solr-index/solr-index.tar.bz2 --strip-components=4" 
  03-ls-var-app-cur:
    command: "ls -l /var/app/solr-index"
